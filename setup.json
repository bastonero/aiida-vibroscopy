{
    "name": "aiida-quantumespresso-vibroscopy",
    "author": "Lorenzo Bastonero",
    "author_email": "bastonero.lorenzo@gmail.com",
    "url": "https://github.com/bastonero/aiida-quantumespresso-vibroscopy",
    "description": "AiiDA plugin to compute vibrational spectra and phonon properties using the Quantum ESPRESSO code.",
    "license": "MIT",
    "classifiers": [
        "Programming Language :: Python",
        "Intended Audience :: Science/Research",
        "License :: OSI Approved :: MIT License",
        "Natural Language :: English",
        "Framework :: AiiDA"
    ],
    "version": "0.1.0",
    "entry_points": {
        "aiida.data": [
            "quantumespresso.vibroscopy.fp = aiida_quantumespresso_vibroscopy.data.vibro_fp: VibrationalFrozenPhononData",
            "quantumespresso.vibroscopy.lr = aiida_quantumespresso_vibroscopy.data.vibro_lr: VibrationalLinearResponseData"
        ],
        "aiida.workflows": [
            "quantumespresso.vibroscopy.dielectric = aiida_quantumespresso_vibroscopy.workflows.dielectric.base: DielectricWorkChain",
            "quantumespresso.vibroscopy.dielectric.numerical_derivatives = aiida_quantumespresso_vibroscopy.workflows.dielectric.numerical_derivatives: NumericalDerivativesWorkChain",
            "quantumespresso.vibroscopy.phonons.harmonic = aiida_quantumespresso_vibroscopy.workflows.phonons.harmonic: HarmonicWorkChain",
            "quantumespresso.vibroscopy.spectra.iraman = aiida_quantumespresso_vibroscopy.workflows.spectra.iraman: IRamanSpectraWorkChain",
            "quantumespresso.vibroscopy.spectra.intensities_average = aiida_quantumespresso_vibroscopy.workflows.spectra.intensities_average: IntensitiesAverageWorkChain"
        ]
    },
    "setup_requires": ["reentry"],
    "install_requires": [
        "aiida-core>=1.6.0",
        "aiida-quantumespresso>=3.4.0",
        "aiida-phonopy>=0.7",
        "quadpy>=0.16.10"
    ],
    "extras_require": {
        "tests": [
            "pgtest~=1.3.1",
            "wheel~=0.31",
            "coverage",
            "pytest~=6.0",
            "pytest-cov"
        ],
        "pre-commit": [
            "pre-commit~=2.2",
            "pylint>=2.5.0,<2.9"
        ],
        "docs": [
            "Sphinx",
            "docutils",
            "sphinx-copybutton~=0.3.0",
            "sphinx-book-theme~=0.1.0",
            "sphinx-click~=2.7.1"
        ]
    }
}
